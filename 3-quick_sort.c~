#include "sort.h"

/**
 * swap_array - common function to swap two array elements
 * @array: Array to manipulate
 * @s: Size of array
 * @a: index of first element
 * @b: index of second element
 */
void swap_array(int *array, size_t s, int a, int b)
{
	int t = array[a];

	array[a] = array[b];
	array[b] = t;
	print_array(array, s);
}

